{
    "tabWidth": 4,
    "useTabs": true,
    "singleQuote": false,
    "trailingComma": "none",
    "bracketSameLine": true,
    "semi": true,
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "auto",
    "importOrder": ["^@core/(.*)$", "^@server/(.*)$", "^@ui/(.*)$", "^[./]"],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "overrides": []
}
